{
    "name": "juryrigged",
    "version": "0.1.0",
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "tsx watch src/server.ts",
        "dev:dashboard": "vite",
        "lint": "tsc --noEmit",
        "build": "tsc -p tsconfig.json && vite build",
        "build:dashboard": "vite build",
        "test": "node --import tsx --test 'src/**/*.test.ts'",
        "test:ops": "node --import tsx --test src/ops/ops-config.test.ts",
        "smoke:staging": "bash ./scripts/staging-smoke.sh",
        "start": "node dist/server.js",
        "migrate": "tsx src/scripts/migrate.ts",
        "record:sse": "tsx src/scripts/record-sse-fixture.ts",
        "migrate:dist": "node dist/scripts/migrate.js",
        "docker:up": "docker compose up --build",
        "docker:down": "docker compose down"
    },
    "dependencies": {
        "dotenv": "^17.2.3",
        "express": "^4.21.2",
        "express-rate-limit": "^8.2.1",
        "howler": "^2.2.4",
        "pixi.js": "^8.0.0",
        "prom-client": "^15.1.3",
        "postgres": "^3.4.5",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "tmi.js": "^1.8.5"
    },
    "devDependencies": {
        "@types/express": "^4.17.21",
        "@types/howler": "^2.2.11",
        "@types/node": "^22.13.9",
        "@types/react": "^18.3.12",
        "@types/react-dom": "^18.3.1",
        "@vitejs/plugin-react": "^4.3.4",
        "autoprefixer": "^10.4.20",
        "postcss": "^8.4.49",
        "tailwindcss": "^3.4.17",
        "tsx": "^4.20.5",
        "typescript": "^5.9.3",
        "vite": "^6.0.7"
    }
}
